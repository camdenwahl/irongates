<link rel='stylesheet' href='/stylesheets/style.css' />


<h1>Manage Users</h1>
<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Actions</th>
  </tr>
  <% users.forEach(function(user) { %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td>
      <a href="/admin/users/edit/<%= user._id %>">Edit</a>
      <a href="/admin/users/delete/<%= user._id %>">Delete</a>
    </td>
  </tr>
  <% }); %>
</table>
